{
    "mcpServers": {
        "context7": {
            "command": "bunx",
            "args": [
                "@upstash/context7-mcp"
            ]
        },
        "docs-manager": {
            "command": "bunx",
            "args": [
                "mcp-docs-service",
                "."
            ]
        },
        "mcp-sequentialthinking-tools": {
            "command": "bunx",
            "args": [
                "mcp-sequentialthinking-tools"
            ]
        },
        "git": {
            "command": "bunx",
            "args": [
                "@cyanheads/git-mcp-server"
            ]
        },
        "lsp-toml": {
            "command": "mcp-language-server",
            "args": [
                "--workspace",
                ".",
                "--lsp",
                "taplo",
                "lsp",
                "stdio"
            ]
        },
        "lsp-yaml": {
            "command": "mcp-language-server",
            "args": [
                "--workspace",
                ".",
                "--lsp",
                "bunx",
                "yaml-language-server",
                "--stdio"
            ]
        },
        "lsp-ast-grep": {
            "command": "mcp-language-server",
            "args": [
                "--workspace",
                ".",
                "--lsp",
                "uvx",
                "--from",
                "ast-grep-cli",
                "ast-grep",
                "lsp"
            ]
        }
    }
}
